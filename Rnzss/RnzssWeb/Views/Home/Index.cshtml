@{
    ViewBag.Title = "RNZSS";
}
<table border="0" class="page-container">
    <tr>
        <td colspan="3" class="main-page-image-td">
            <div id="image-head" class="main-page-image-div">
                <div class="centering-hack">
                    @*<h1>Something HTML</h1> Uncomment this if you want to add text to picture*@
                </div>
            </div>
        </td>
    </tr>
    <tr>
        <td colspan="3" @*style="background-color: blueviolet;"*@>
            <table border="0" class="main-page-boottom-table" >
                <tr>
                    <td width="20%;"></td>
                    <td class="main-page-td-containing-small-box">
                        <div id="naval" class="main-page-small-box">
                            <p>
                                <h3 class="text-on-gray-background">Government Support</h3>
                                The right part.<br />
                                The right price.<br />
                                The right time.<br />

                                Request a New Quote
                                Inventory Search
                                Explore Contract Types                                </p>

                        </div>
                    </td>
                    <td class="main-page-space-between-small-box-td"></td>
                    <td style="min-width:300px;">
                        <div id="armoredvehicles" class="main-page-small-box">
                            <h3 class="text-on-gray-background"> MIL-SPEC Packaging & Shipping</h3>

                                Complete in-house MIL-SPEC Packaging and certified Quality Department.
                                    
                                <br />
                                Quality Control
                                <br />
                                Custom Packaging & Crafting
                                <br />
                                Warehousing & Shipping
                                <br />
                                WAWF-EDI
                                <br />
                        </div>
                    </td>
                    <td class="main-page-space-between-small-box-td"></td>
                    <td style="min-width:300px;">
                        <div id="aerospace" class="main-page-small-box">
                            <h3 class="text-on-gray-background">Products</h3>
                            Search thousands of NSNs and purchase online.
                            Inventory Search
                            <br />
                            Aerospace
                            <br />
                            Electrical
                            <br />
                            Fleet & Ground
                            <br />
                            Hardware
                            <br />
                            Hydraulics & Pneumatics
                            Mechanical
                            Metals & Fabricated Products
                        </div>
                    </td>
                    <td class="main-page-space-between-small-box-td"></td>
                    <td style="min-width:180px;">
                        <button id="contactUsButton" type="button" class="button"> Contact us </button>
                        <br />
                        <br />
                    </td>
                    <td width="20%;"></td>
                </tr>
              
            </table>
        </td>
    </tr>
</table>






<script>
        $(document).ready(function () {
            var url = [
                    '../../Content/Images/MainPage1.jpg',
                    '../../Content/Images/MainPage2.jpg',
                    '../../Content/Images/MainPage3.jpg',
                    '../../Content/Images/MainPage4.jpg',
                    '../../Content/Images/MainPage5.jpg',
                    '../../Content/Images/MainPage6.jpg',
                    '../../Content/Images/MainPage7.jpg',
                    '../../Content/Images/MainPage8.jpg',
                    '../../Content/Images/MainPage9.jpg',
                    '../../Content/Images/MainPage10.jpg',
                    '../../Content/Images/MainPage11.jpg',
                    '../../Content/Images/MainPage12.jpg'
        ];

            var curentImageIndex = 0;

            setInterval(function () {
                //console.log(url[curentImageIndex])
                var p = $('.main-page-image-div');
                p.css("background-image", "url(" + url[curentImageIndex++] + ")");
                p.css("background-repeat", "no-repeat");
                if (curentImageIndex >= url.length) { curentImageIndex = 0 }
            }, 8000);
        });


        $('#naval').click(function (e) {
            window.location.href = '@Url.Action("Naval", "Home")';
        });
        $('#armoredvehicles').click(function (e) {
            window.location.href = '@Url.Action("ArmoredVehicles", "Home")';
        });
        $('#aerospace').click(function (e) {
            window.location.href = '@Url.Action("AeroSpace", "Home")';
        });
        $('#contactUsButton').click(function (e) {
            window.location.href = '@Url.Action("Contact", "Home")';
        });



</script>


